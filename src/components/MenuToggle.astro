---
const bars = (new Array(3) as any).fill();
---

<ul class="space-y-3 w-12">
  {
    bars.map((_bar: any, index: number) => {
      return (
        <li
          class={`h-1 w-12 bg-gray-700 rounded-xs transition-all menu-bar menu-bar-${
            index + 1
          }`}
        />
      );
    })
  }
</ul>
